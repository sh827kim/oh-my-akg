{
  "name": "@archi-navi/cli",
  "version": "0.1.0",
  "private": false,
  "description": "CLI tool for Archi.Navi",
  "bin": {
    "archi-navi": "./bin/archi-navi.js"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@archi-navi/core": "workspace:*",
    "@archi-navi/db": "workspace:*",
    "@archi-navi/inference": "workspace:*",
    "@archi-navi/shared": "workspace:*",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "drizzle-orm": "^0.45.1",
    "ora": "^9.3.0"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
