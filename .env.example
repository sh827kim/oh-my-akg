# === DB ===
# Local-first (PGlite) - 기본값, 별도 설정 불필요
# PostgreSQL 연결 시 아래 주석 해제
# DATABASE_URL=postgresql://postgres:password@localhost:5432/archinavi

# PGlite 데이터 저장 경로 (기본: .archi-navi/data)
PGLITE_DATA_DIR=.archi-navi/data

# === AI ===
# 프로바이더: openai | anthropic | google
AI_PROVIDER=openai

# AI API 키 (필수)
AI_API_KEY=

# 사용할 모델
AI_MODEL=gpt-4o

# === App ===
NODE_ENV=development
PORT=3000
NEXT_PUBLIC_APP_URL=http://localhost:3000
